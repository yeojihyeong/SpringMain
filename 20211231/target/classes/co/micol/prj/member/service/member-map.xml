<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.micol.prj.member.service.MemberMapper">

	<select id="memberSelectList" resultType="co.micol.prj.member.service.MemberVO">
		SELECT * FROM MEMBER
	</select>

	<select id="memberSelect" resultType="co.micol.prj.member.service.MemberVO">
	SELECT * FROM MEMBER
	WHERE ID = #{id}
	<if test="password != null">
		AND PASSWORD = #{password}
	</if>	
	</select>
	
	<insert id="memberInsert">
	
	</insert>
	
	<delete id="memberDelete">
	
	</delete>
	
	<update id="memberUpdate">
	
	</update>
	
	<select id="isIdCheck">
	
	</select>
</mapper>